package com.mr.chapter04;

public class Ex03 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根

	}
	
	class Animal
	{
		String name="动物";
		
		//定义父类方法
		void shout()
		{
			System.out.println("动物发出叫声");
		}
	}
	
	class Dog extends Animal
	{
		//重写父类变量
		String name="犬类";
		//访问父类方法
		void shout()
		{
			super.shout();
		}
		void printName()
		{
			
		}
	}

}
