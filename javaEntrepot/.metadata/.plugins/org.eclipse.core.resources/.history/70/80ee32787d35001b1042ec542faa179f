package com.mr.chapter06;

public class Demo {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根

	}

}

class Person 
{
	String name;
	int age;
	
	public void Person(String name,int age)
	{
		this.name = name;
		this.age = age;
	}
	
	public int hashCode(){
		return name.hashCode();
	}
	
	public boolean equals(Object obj)
	{
		Person p = (Person)obj;
		if(this.name == p.name)
		{
			return true;
		}
	}
}
