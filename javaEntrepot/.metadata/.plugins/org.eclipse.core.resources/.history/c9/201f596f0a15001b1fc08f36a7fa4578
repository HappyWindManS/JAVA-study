package com.mr.chapter03;

public class Ex06 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根

	}
	
	public static int add(int x,int y)
	{
		return x+y;
	}
	public static int add(int x,int y,int z)
	{
		return x+y+z;
	}
	public static double add(double x,double y)
	{
		return x+y;
	}
	
}
