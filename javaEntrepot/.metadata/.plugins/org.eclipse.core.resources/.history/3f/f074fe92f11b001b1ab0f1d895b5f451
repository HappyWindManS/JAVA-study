package arrays;

import java.util.Scanner;

public class EX01 {

	public static void main(String[] args){
		// TODO 自动生成的方法存根
		Scanner st = new Scanner(System.in);
		int[] array = new int[10];
		for(int i=0;i<10;i++)
		{		
			array[0] = st.nextInt();
		}
		
		for(int i=0;i<10;i++)
		{
			System.out.println(array[i]);
		}
		
		int num = st.nextInt();
		int time = 0;
		for(int i=0;i<10;i++)
		{
			if(array[i] <= num+30)
			{
				time++;
			}
		}
		System.out.print(time);
	}
}
