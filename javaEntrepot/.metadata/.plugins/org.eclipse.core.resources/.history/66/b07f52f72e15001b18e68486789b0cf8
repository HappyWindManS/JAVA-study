public class Ex05 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根

	}
	class Animal
	{
		public Animal()
		{
			System.out.println("我是一只动物");
		}
		public Animal(String name)
		{
			System.out.println("我是一只"+name);
		}
		
	}
	
	class Dog extends Animal
	{
		public Dog()
		{
			
		}
	}
}
