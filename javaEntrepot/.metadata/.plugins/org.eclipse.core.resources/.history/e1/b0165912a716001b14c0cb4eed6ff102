package arithmetic1025;

import java.util.ArrayList;

public class PrimeNumber {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		
		/*
		 *已知寻找素数范围301-400
		 *大于23*23的数，如果不是素数，都将被23以下素数整除*/

		
		//定义素数数组
		ArrayList<Integer> arr = new ArrayList<Integer>();
		//寻找21以下质数,存入数组
		for(int i=1;i<21;i++)
		{
			for(int l=1;l<i/2+1;l++)
			{
				if(i%l==0)
				{
					break;
				}
				arr.add(i);
			}
		}
		
	}

}
