package com.mr.chapter03;

public class Ex10 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Person p1= new Person();
		
	}

}

class Person
{
	String name;
	int age;
	public Person(int age)
	{
		age = age;
	}
	
	public Person(String name,int age)
	{
		this.name = name;
		this.age =age;
	}
	
	public void speak ()
	{
		System.out.println("我今年"+age+"岁了！");
	}
	
	public void say()
	{
		System.out.println("我叫"+name+",我今年"+age+"岁了！");
	}
}