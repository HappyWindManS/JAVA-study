package chapter02;

public class Ex09 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int grade = 75;
        if(grade>80){
        	//如果满足条件grade》80
        	System.out.println("该成绩为优");
        }else if(grade>70){
        	//如果不满足条件grade》80，但满足条件grade》70
        	System.out.println("该成绩为良");
        }else if(grade>60){
        	//如果不满足条件grade》70，但满足条件grade》60
        	System.out.println("该成绩为中");
        }else{
        	//如果不满足条件grade》60
        	System.out.println("该成绩为差");
        }
	}

}
