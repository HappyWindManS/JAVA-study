package com.mr.chapter04;

public class Ex02 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Ex02 e = new Ex02();
		
	}
	
	class Animal
	{
		void shout()
		{
			System.out.println("动物发出叫声");
		}
	}
	
	class Dog extends Animal
	{
		//子类方法
		void shout()
		{
			System.out.println("汪汪汪.......");
		}
	}
}
