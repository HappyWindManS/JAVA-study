package test;

import java.util.Scanner;

public class There {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Scanner input = new Scanner(System.in);
		int[] array = new int[3];
		for(int i = 0;i<3;i++)
		{
			array[i]=input.nextInt();
		}
		
		int temp = 0;
		while(array[0]>array[2]&&array[0]<array[1]&&array[1]>array[2])
		{
			if(array[0]>array[1])
			{
				temp = array[1];
				array[1]=array[0];
				array[0]=temp;
			}
			if(array[2]>array[1])
			{
				temp = array[1];
				array[1]=array[2];
				array[2]=temp;
			}
		}
		
		System.out.println();
	}

}
