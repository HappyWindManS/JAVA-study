package com.mr.chapter04;

public class Ex02 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Ex02 e = new Ex02();
		Dog dog = e.new Dog();
		dog.shout();
	}
	
	//父类
	class Animal
	{
		void shout()
		{
			System.out.println("动物发出叫声");
		}
	}
	
	//子类
	class Dog extends Animal
	{
		//子类中重新声明新的方法
		//子类方法
		void shout()
		{
			System.out.println("汪汪汪.......");
		}
	}
}
