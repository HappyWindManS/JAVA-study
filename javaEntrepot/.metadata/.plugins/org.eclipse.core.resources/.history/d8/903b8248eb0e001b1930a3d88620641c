package chapter02;

public class Ex02 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        byte a ;//定义byte类型的变量a
        int b = 298;//定义int变量b的值为298
        a = (byte) b;
        System.out.println("b="+b);
        System.out.println("a="+a);
 	}

}
