package com.mr.chapter04;

//当父类未定义无参构造方法时，子类不能声明无参构造方法

public class Ex05 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Dog dog=new Dog();
	}
}

class Animal
{
	public Animal()
	{
		System.out.println("我是一只动物");
	}
	public Animal(String name)
	{
		System.out.println("我是一只"+name);
	}
	
}

class Dog extends Animal
{
	public Dog()
	{
		
	}
}