package com.mr.chapter04;

public class Ex04 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Ex04 e =new Ex04();
		e.new Dog();
	}
	
	class Animal
	{
		public Animal(String name)
		{
			System.out.println("我是一只"+name);
		}
	}
	
	class Dog extends Animal
	{
		public Dog()
		{
			//super代指父类
			//调用父类的构造方法
			super("沙皮狗");
		}
	}
}
