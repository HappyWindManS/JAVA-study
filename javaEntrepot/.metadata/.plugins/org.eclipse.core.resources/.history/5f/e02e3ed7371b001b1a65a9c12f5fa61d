package rudiment;

import java.util.Scanner;

public class EX07 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Scanner input = new Scanner(System.in);
		//获取第几位数
		int num = input.nextInt();
		//判断在第几列
		int line = 0;	
		//记录
		int temp = 0;
		//把这个数在第几列给标出来
		int i=1;
		for(;temp<=num;i++)
		{
			temp += i;
			line = i;
		}
		//判断在第几列的第几个数
		int time = num + i - temp -1;
		
		if(line % 2 ==0)
		{
			if(time<=line/2)
			{
				System.out.print(time+"/"+(line - time +10));
			}
			else
			{
				time -= line/2;
				System.out.print((line - time +10)+"/"+time);
			}
		}
		else
		{
			if(time<=line/2)
			{
				time -= line/2;
				System.out.print((line - time +10)+"/"+time);
			}
			else
			{
				System.out.print(time+"/"+(line - time +10));
			}
		}

		
	}
}
