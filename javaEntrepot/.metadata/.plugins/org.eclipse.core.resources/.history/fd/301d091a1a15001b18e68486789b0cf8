public class Ex01 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Ex01 e = new Ex01();
		Dog dog = new Dog();
		
	}
	
    class Animal
	{
		String name;
		void shout()
		{
			System.out.println("动物发出叫声");
		}
	}
	
	class Dog extends Animal
	{
		public void printName()
		{
			System.out.println("name="+name);
		}
	}
