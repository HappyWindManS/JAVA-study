package com.mr.chapter06;

import java.util.HashMap;
import java.util.Map;

public class Ex18 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Map map1 = new HashMap();
		map1.put("1", "Jack");
		map1.put("2", "Rose");
		map1.put("3", "Lucy");
		map1.forEach((k,v)-> System.out.println(k+":"+v));
		System.out.println("");
	}

}
